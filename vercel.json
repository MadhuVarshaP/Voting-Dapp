{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "CI=false npm run build",
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ]
}